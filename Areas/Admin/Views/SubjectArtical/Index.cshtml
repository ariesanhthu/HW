@model IEnumerable<SubjectArtical>

@{
    ViewData["Title"] = "Môn học";
}

<br/>

<h1>Môn học</h1>

<div class="col-3">
    <a asp-action="Upsert" asp-controller="SubjectArtical" class="btn btn-info form-control text-white">Thêm môn học</a>
</div>
<br/>
<div class="col-12 border p-3">
        <table id = "DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Tên môn học</th>
                    <th>Tổng số bài viết</th>
                    <th></th>
                </tr>
            </thead>
        </table>
</div>

@section Scripts{
    <script src = "~/js//SubjectArticalSite.js"></script>
}